{
  "name": "payment",
  "version": "2.3.0",
  "description": "A general purpose library for building credit card forms, validating inputs and formatting numbers. Base on jquery.payment by @stripe, but without the jQuery.",
  "keywords": [
    "payment",
    "cc",
    "card"
  ],
  "author": "Jesse Pollak",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/jessepollak/payment.git"
  },
  "main": "lib/index.js",
  "scripts": {
    "compile": "coffee -o lib/ --compile ./src/*.coffee",
    "build": "cross-env NODE_ENV=production webpack",
    "development": "coffee -o lib/ --compile --watch ./src/index.coffee",
    "preversion": "npm run test",
    "postpublish": "git push gaarlos master; git push --tags",
    "test": "karma start --single-run --browsers PhantomJS"
  },
  "devDependencies": {
    "browserify": "~16.2.3",
    "coffee-loader": "^0.9.0",
    "coffee-script": "~1.12",
    "coffeescript": "^2.4.1",
    "cross-env": "^5.2.0",
    "jsdom": "~14.0",
    "json-loader": "^0.5.7",
    "karma": "^4.0.1",
    "karma-chai": "^0.1.0",
    "karma-chai-plugins": "^0.9.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-coverage": "^1.1.2",
    "karma-mocha": "^1.3.0",
    "karma-phantomjs-launcher": "^1.0.4",
    "karma-sinon": "^1.0.5",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^3.0.5",
    "mocha": "~6.1",
    "mocha-webpack": "^1.1.0",
    "mversion": "^1.13.0",
    "nodemon": "~1.18.11",
    "phantomjs-prebuilt": "^2.1.16",
    "run-sequence": "~2.2.1",
    "tiny-lr": "1.1.1",
    "vinyl-source-stream": "~2.0.0",
    "webpack": "^4.29.6"
  },
  "dependencies": {
    "qj": "~2.0.0"
  }
}
